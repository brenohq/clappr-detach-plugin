    "Clappr"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
