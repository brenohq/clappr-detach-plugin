    "css-loader": "0.23.1",
    "eslint": "^4.6.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "html-loader": "0.4.3",
    "interact.js": "1.2.6",
    "lodash.assign": "^4.2.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
